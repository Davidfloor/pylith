# -*- Python -*- (syntax highlighting)
# ----------------------------------------------------------------------
# Create blocks for materials
# ----------------------------------------------------------------------
block 1 surface slab_material plate_material
block 1 name "slab_material"

block 2 surface plate_material
block 2 name "plate_material"

block 3 surface wedge_material
block 3 name "wedge_material"

# ----------------------------------------------------------------------
# Create nodeset for fault
# ----------------------------------------------------------------------
group "fault" add node in fault_surface_top
group "fault" add node in fault_surface_bot
nodeset 10 group fault
nodeset 10 name "fault"

# ----------------------------------------------------------------------
# Create nodeset for fault edge
# ----------------------------------------------------------------------
group "fault_end" add node in vertex vertex_fault_bot
nodeset 11 group fault_end
nodeset 11 name "fault_end"

# ----------------------------------------------------------------------
# Create nodeset for splay
# ----------------------------------------------------------------------
group "splay" add node in splay_surface
nodeset 12 group splay
nodeset 12 name "splay"

# ----------------------------------------------------------------------
# Create nodeset for splay edge
# ----------------------------------------------------------------------
group "splay_end" add node in vertex vertex_splay_bot
nodeset 13 group splay_end
nodeset 13 name "splay_end"

# ----------------------------------------------------------------------
# Create nodeset for +x edge
# ----------------------------------------------------------------------
group "boundary_xpos" add node in curve curve_xpos
nodeset 20 group boundary_xpos
nodeset 20 name "boundary_xpos"

# ----------------------------------------------------------------------
# Create nodeset for -x edge
# ----------------------------------------------------------------------
group "boundary_xneg" add node in curve curve_xneg_hw
group "boundary_xneg" add node in curve curve_xneg_fw
nodeset 21 group boundary_xneg
nodeset 21 name "boundary_xneg"

# ----------------------------------------------------------------------
# Create nodeset for +y edge
# ----------------------------------------------------------------------
group "boundary_ypos" add node in curve ground_fw ground_hw ground_wedge
nodeset 22 group boundary_ypos
nodeset 22 name "boundary_ypos"

# ----------------------------------------------------------------------
# Create nodeset for -y edge
# ----------------------------------------------------------------------
group "boundary_yneg" add node in curve curve_yneg
nodeset 23 group boundary_yneg
nodeset 23 name "boundary_yneg"


# End of file
